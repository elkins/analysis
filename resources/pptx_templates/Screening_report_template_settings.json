{
    "_metadata": {
        "PPTxTemplateName": "Screening_report_template",
        "mapperClass": "ScreeningReportTemplateMapper",
        "templateMapperName": "Screening PPTx Report",
        "ApplicationVersion": "3.2.12"
    },
    "max_axes_per_slide": 4,
    "region_data_points_width": 100,
    "plot_settings": {
        "font": {
            "family": "Helvetica",
            "size": 6
        },
        "spectrum_line": {
            "linewidth": 0.5
        },
        "tick_params": {
            "axis": "x",
            "labelsize": 4,
            "width": 0.5
        },
        "xlabel": {
            "xlabel": "[ppm]",
            "fontsize": 4,
            "ha": "right",
            "x": 1.0,
            "labelpad": -15
        },
        "peak_symbol": {
            "marker": "x",
            "s": 10,
            "alpha": 0.8,
            "linewidths": 0.3
        },
        "peak_legend_settings": {
            "loc": 1,
            "fontsize": 4,
            "ncol": 1,
            "numpoints": 3,
            "frameon": false
        },
        "spine_width": 0.2,
        "dpi": 300,
        "format": "png",
        "show_peak_labels": true,
        "show_peak_symbols": true
    }
}